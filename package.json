{
  "name": "@digitaldefiance/i18n-lib",
  "version": "4.1.2",
  "description": "i18n library with enum translation support",
  "homepage": "https://github.com/Digital-Defiance/i18n-lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Digital-Defiance/i18n-lib.git"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "npx nx build digitaldefiance-i18n-lib",
    "build:prod": "NODE_ENV=production npx nx build digitaldefiance-i18n-lib --configuration=production",
    "build:stream": "npx nx build --outputStyle=stream digitaldefiance-i18n-lib",
    "build:logged": "npx nx build --outputStyle=stream --skip-nx-cache digitaldefiance-i18n-lib 2>&1 | ansifilter -o build.log",
    "test": "npx nx test digitaldefiance-i18n-lib",
    "test:stream": "npx nx test --outputStyle=stream digitaldefiance-i18n-lib",
    "test:logged": "npx nx test --skip-nx-cache --outputStyle=stream digitaldefiance-i18n-lib 2>&1 | ansifilter -o test.log",
    "test:coverage": "npx jest --coverage --testPathPattern=digitaldefiance-i18n-lib --coverageReporters=text",
    "lint": "npx nx lint digitaldefiance-i18n-lib",
    "lint:fix": "npx nx lint digitaldefiance-i18n-lib --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}'",
    "format": "npx nx format:write --projects=digitaldefiance-i18n-lib",
    "format:check": "npx nx format:check --projects=digitaldefiance-i18n-lib",
    "prepublishOnly": "npx nx build digitaldefiance-i18n-lib",
    "publish:public": "cd ../../dist/packages/digitaldefiance-i18n-lib && npm publish --access public"
  },
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "i18n",
    "internationalization",
    "typescript",
    "enum",
    "translation"
  ],
  "author": "Digital Defiance",
  "license": "MIT",
  "packageManager": "yarn@4.10.3",
  "dependencies": {
    "@digitaldefiance/branded-enum": "0.0.6",
    "currency-codes": "^2.2.0",
    "lru-cache": "^5.1.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-check": "^4.3.0"
  }
}
